{
  "name": "NameMC Extras",
  "description": "An extension that enhances your NameMC experience with a range of powerful features.",
  "author": "Faav",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 3,
  "version": "3.9.4",
  "content_scripts": [
    {
      "js": [
        "js/url-cleaner.js",
        "js/data-preload.js",
        "js/user-data-utils.js",
        "js/announcement.js",
        "js/page-injector.js"
      ],
      "css": [
        "css/NameMC.css",
        "css/announcement.css",
        "css/pinned.css"
      ],
      "matches": [
        "*://*.namemc.com/*"
      ],
      "run_at": "document_start"
    },
    {
      "css": [
        "css/CustomPage.css"
      ],
      "matches": [
        "*://*.namemc.com/extras/*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/extras/skin-cape-test*/*",
        "*://*.namemc.com/extras/badges*/*",
        "*://*.namemc.com/extras/badge/*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/badge-page/badge-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/extras/badge/*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/extras/badge/*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/capes-page/capes-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/capes*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/capes*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/capes-page/official-cape-page/official-cape-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/cape/*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/cape/*/*"
      ],
      "run_at": "document_start"
    },
    {
      "css": [
        "css/CustomPage.css"
      ],
      "js": [
        "js/capes-page/custom-cape-page/custom-cape-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/cape/*/*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/cape/*/*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/skin-page/official-skin-page/official-skin-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/skin/*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/skin/*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/profile-page/profile-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/profile/*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/profile/*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/my-profile-page/names-page/names-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/my-profile/names*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/my-profile/names*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/my-profile-page/capes-page/capes-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/my-profile/capes*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/my-profile/capes*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/my-profile-page/emoji-page/emoji-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/my-profile/emoji*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/my-profile/emoji*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/my-profile-page/skins-page/skins-injector.js"
      ],
      "matches": [
        "*://*.namemc.com/my-profile/skins*"
      ],
      "exclude_matches": [
        "*://*.namemc.com/my-profile/skins*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/*"
      ],
      "matches": [
        "*://*.namemc.com/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://data.faav.top/*",
    "https://bedrockviewer.com/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{499f5f73-58cb-4eb9-a320-127f42cccb5e}"
    }
  }
}