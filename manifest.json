{
  "name": "NameMC Extras",
  "description": "By Faav#6320",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 3,
  "version": "2.0",
  "content_scripts": [
    {
      "js": [
        "js/injector.js"
      ],
      "css": [
        "css/NameMC.css"
      ],
      "matches": [
        "*://*.namemc.com/profile/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["js/namemc-inject.js"],
    "matches": ["<all_urls>"]
  }]
}
