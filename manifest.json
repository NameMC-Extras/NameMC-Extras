{
  "name": "NameMC Extras (Rewrite)",
  "description": "By Faav#6320",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "version": "0.0.1",
  "content_scripts": [
    {
      "css": [
        "css/NameMC.css"
      ],
      "matches": [
        "*://*.namemc.com/*"
      ]
    },
    {
      "js": [
        "js/injector.js"
      ],
      "matches": [
        "*://*.namemc.com/profile/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["js/namemc-inject.js"]
}
